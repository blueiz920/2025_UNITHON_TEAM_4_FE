{
  "version": 2,
  "functions": {
    "api/proxy.ts": {
      "memory": 512,
      "maxDuration": 10,
      "runtime": "nodejs20.x",
      "bodyParser": false
    }
  },
  "rewrites": [
    {
      "source": "/api/proxy",
      "destination": "/api/proxy.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "source": "/.*",
      "destination": "/index.html"
    }
  ]
}
